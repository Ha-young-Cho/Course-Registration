<h1>Student 데이터 삽입</h1>

<form name="student" method="post" action="/">
    <div>
        <label for="s_id">S_id</label>
        <input id="s_id" name="s_id" type="text"  required placeholder="S_id">
    </div>
    <div>
        <label for="s_name">S_name</label>
        <input id="s_name" name="s_name" type="text" placeholder="S_name">
    </div>
    <div>
        <label for="s_email">S_email</label>
        <input id="s_email" name="s_email" type="text" required placeholder="S_email">
    </div>
    <div>
        <label for="major">Major</label>
        <input id="major" name="major" type="text" placeholder="Major">
    </div>
    <div>
        <label for="d_id">D_id</label>
        <input id="d_id" name="d_id" type="text"  placeholder="D_id">
    </div>
    <div>
        <label for="s_phone_number">S_phone_number</label>
        <input id="s_phone_number" name="s_phone_number" type="text"  placeholder="S_phone_number">
    </div>
    <div>
        <input type ="submit" value ="삽입">
    </div>
</form>